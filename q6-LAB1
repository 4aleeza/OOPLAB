#include <iostream>
#include <string>
using namespace std;
int main() {
	string name;
	float sum = 0;
	float avg = 0;
	float score[5];
	getline(cin, name);
	for (int i = 0;i < 5;i++) {
		cin >> score[i];
		sum = sum + score[i];
	}
	avg = sum / 5;
	cout << "Student Name: " << name << endl;
	cout << "Test Scoress: " << score[0] << " " << score[1] << " " << score[2] << " " << score[3] << " " << score[4] << endl;
	cout << "Average Score: " << avg;
}
